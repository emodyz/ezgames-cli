{
  "name": "ezgames-cli",
  "description": "EZGames CLI Toolkit",
  "version": "0.0.0",
  "author": "Saber Nouira @iWirk",
  "bin": {
    "ezgames": "./bin/run"
  },
  "bugs": "https://github.com/ezmodyz/ezgames-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1.3.4",
    "@oclif/plugin-autocomplete": "^0.3.0",
    "@oclif/plugin-help": "^3",
    "@octokit/request": "^5.4.14",
    "axios": "^0.21.1",
    "chalk": "^4.1.0",
    "cli-table": "^0.3.6",
    "cli-ux": "^5.5.1",
    "collect.js": "^4.28.6",
    "enquirer": "^2.3.6",
    "envfile": "^6.14.0",
    "execa": "^5.0.0",
    "figures": "^3.2.0",
    "fs-extra": "^9.1.0",
    "listr2": "^3.4.1",
    "minimist": "^1.2.5",
    "moment": "^2.29.1",
    "retry-axios": "^2.4.0",
    "tslib": "^1",
    "validator": "^13.5.2",
    "which": "^2.0.2",
    "xdg-app-paths": "^7.3.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/cli-table": "^0.3.0",
    "@types/fs-extra": "^9.0.8",
    "@types/inquirer": "^7.3.1",
    "@types/minimist": "^1.2.1",
    "@types/mocha": "^5",
    "@types/node": "^14.14.31",
    "@types/validator": "^13.1.3",
    "@types/which": "^2.0.0",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/ezmodyz/ezgames-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "ezgames",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete"
    ]
  },
  "repository": "ezmodyz/ezgames-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc.yml",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
